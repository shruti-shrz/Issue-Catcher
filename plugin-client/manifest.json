{
"manifest_version": 2,
"name": "Github_plugin",
"version": "1.0",
"description": "Build an Extension!",
"permissions": ["storage"],
"background":{
"scripts":["background.js","jquery.js"]
},
"content_scripts":[
{
	"matches" : ["https://github.com/*"],
	"js":["content.js"]
}
],
"browser_action": {
	"default_popup": "popup.html",
	"default_title": "Check the similar issue here."
}

}